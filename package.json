{
  "name": "ts-react-chrome-extension",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "parcel": "^2.8.3",
    "parcel-plugin-static-files-copy": "^2.4.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typescript": "^3.9.5"
  },
  "scripts": {
    "build": "sh build.sh",
    "start": "yarn run nodemon --exec yarn run build"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.115",
    "nodemon": "^2.0.22"
  },
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "dist/",
      ".cache/"
    ],
    "delay": 2500,
    "watch": [
      "react/",
      "html/",
      "images/",
      "./manifest.json"
    ],
    "ext": "ts,tsx,css,json,html,png"
  }
}
